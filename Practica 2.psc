Proceso Practica_1
	Definir n1,n2,n3,m,r Como Real;
	Escribir "Ingresa primer numero";
	Leer n1;
	Escribir "Ingresa segundo numero";
	Leer n2;
	Escribir "Ingresa tercer numero";
	Leer n3;
	m<-n1*n2*n3;
	Escribir "La Multiplicacion es",m;
	r<-n1-n2-n3;
	Escribir "La resta es",r;
FinProceso
